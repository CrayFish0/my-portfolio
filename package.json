{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "A responsive, minimalistic portfolio website",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "framer-motion": "^10.16.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "vite": "^4.4.5"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 node ./node_modules/vite/bin/vite.js build",
    "preview": "node ./node_modules/vite/bin/vite.js preview --host 0.0.0.0 --port $PORT",
    "start": "npm run build && npm run preview"
  },
  "homepage": "."
}
